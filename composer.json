{
  "name": "domynation/domynation-framework",
  "description": "Custom microframework built mostly for fun and learning purpose",
  "keywords": [
    "domynation"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Dominique Sarrazin",
      "email": "domynation@gmail.com",
      "homepage": "http://domynation.com/"
    }
  ],
  "autoload": {
    "psr-4": {
      "Domynation\\": "src/Domynation"
    },
    "files": [
      "src/Config/constants.php",
      "src/Helpers/functional.php",
      "src/Helpers/functional.php",
      "src/Helpers/array.php",
      "src/Helpers/util.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "test\\": "Test"
    },
    "classmap": [
      "tests/",
      "src/_very_old_dependencies"
    ]
  },
  "require": {
    "php": ">=7.0.0",
    "symfony/http-foundation": "^2.6",
    "symfony/var-dumper": "^2.7",
    "twig/twig": "~1.0",
    "doctrine/orm": "~2.4",
    "guzzlehttp/guzzle": "~5.2",
    "beberlei/assert": "^2.3",
    "nesbot/carbon": "~1.14",
    "monolog/monolog": "^1.15",
    "php-di/php-di": "^5.1",
    "mathiasverraes/money": "^1.2",
    "ramsey/uuid": "^3.0",
    "erusev/parsedown": "^1.6",
    "predis/predis": "^1.0",
    "elasticsearch/elasticsearch": "~2.0",
    "pusher/pusher-php-server": "^2.2",
    "mailgun/mailgun-php": "1.7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.2",
    "mockery/mockery": "^0.9.4",
    "fzaninotto/faker": "^1.5"
  }
}
